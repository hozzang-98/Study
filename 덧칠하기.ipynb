    "import bisect\n",
    "\n",
    "def solution(n, m, section):\n",
    "    answer = 0\n",
    "    i = 0\n",
    "\n",
    "    while i < len(section):\n",
    "        answer += 1\n",
    "        end = section[i] + m - 1\n",
    "        # end보다 큰 값이 처음 등장하는 인덱스를 찾아서 그걸 i로 설정\n",
    "        i = bisect.bisect_right(section, end)\n",
    "\n",
    "    return answer"
   ]
  }
 ],
