   "source": [
    "def solution(s):\n",
    "    answer = 0\n",
    "    same = 0\n",
    "    diff = 0\n",
    "\n",
    "    for i, char in enumerate(s):\n",
    "        if same == diff:\n",
    "            answer += 1\n",
    "            ref = char  # 기준 문자 갱신\n",
    "        if char == ref:\n",
    "            same += 1\n",
    "        else:\n",
    "            diff += 1\n",
    "\n",
    "    return answer"
   ]
  }
 ],
