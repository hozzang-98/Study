    "import re\n",
    "\n",
    "def solution(babbling):\n",
    "    \n",
    "    pattern = re.compile(r'^(aya|ye|woo|ma)(?!\\1)(aya|ye|woo|ma)*(?!\\2)(aya|ye|woo|ma)*(?!\\3)(aya|ye|woo|ma)*$')\n",
    "    \n",
    "    return sum(1 for b in babbling if pattern.fullmatch(b))"
   ]
  }
