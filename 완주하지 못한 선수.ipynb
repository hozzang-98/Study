   "source": [
    "def solution(participant, completion):\n",
    "    participant.sort()\n",
    "    completion.sort()\n",
    "\n",
    "    for p, c in zip(participant, completion):\n",
    "        if p != c:\n",
    "            return p\n",
    "\n",
    "    return participant[-1]  # 마지막 남은 선수가 완주하지 못한 선수"
   ]
  }
