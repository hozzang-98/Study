    "def solution(a, b, n):\n",
    "    total_cola = 0\n",
    "    \n",
    "    while n >= a:\n",
    "        exchanged = (n // a) * b\n",
    "        total_cola += exchanged\n",
    "        n = exchanged + (n % a)\n",
    "\n",
    "    return total_cola"
   ]
  }
