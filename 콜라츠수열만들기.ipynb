    "def solution(n):\n",
    "    \n",
    "    answer = [n]\n",
    "    \n",
    "    while n != 1:\n",
    "\n",
    "        if n % 2 == 0:\n",
    "            \n",
    "            n /= 2\n",
    "            \n",
    "        else:\n",
    "            \n",
    "            n = 3*n + 1\n",
    "        \n",
    "        answer.append(n)\n",
    "        \n",
    "    return answer"
   ]
  }
