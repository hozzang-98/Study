   "source": [
    "def solution(word):\n",
    "\n",
    "    vowels = \"AEIOU\"  # 모음 리스트\n",
    "    weights = [781, 156, 31, 6, 1]  # 각 자리의 가중치 (5⁴, 5³, 5², 5¹, 5⁰)\n",
    "\n",
    "    index = 0\n",
    "    for i, ch in enumerate(word):  # 단어의 각 문자에 대해 계산\n",
    "        index += vowels.index(ch) * weights[i] + 1\n",
    "\n",
    "    return index"
   ]
  }
