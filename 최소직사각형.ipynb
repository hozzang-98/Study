    "def solution(sizes):\n",
    "\n",
    "    answer = 0\n",
    "\n",
    "    w = max([max(size) for size in sizes])\n",
    "    h = max([min(size) for size in sizes])\n",
    "\n",
    "    answer += w*h\n",
    "\n",
    "    return answer"
   ]
  }
