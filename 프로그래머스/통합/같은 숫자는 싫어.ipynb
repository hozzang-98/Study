   "source": [
    "from collections import deque\n",
    "\n",
    "def solution(arr):\n",
    "    \n",
    "    answer = []\n",
    "    dq = deque(arr)\n",
    "    i = dq.popleft()\n",
    "    answer.append(i)\n",
    "\n",
    "    while dq:\n",
    "\n",
    "        i = dq.popleft()\n",
    "\n",
    "        if i != answer[-1]:\n",
    "\n",
    "            answer.append(i)\n",
    "\n",
    "    return answer"
   ]
  }
