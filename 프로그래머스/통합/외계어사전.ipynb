    "from itertools import permutations\n",
    "\n",
    "def solution(spell, dic):\n",
    "\n",
    "    answer = 2\n",
    "\n",
    "    sp = [\"\".join(myset) for myset in list(permutations(spell))]\n",
    "\n",
    "    for word in sp:\n",
    "\n",
    "        if word in dic:\n",
    "            \n",
    "            answer = 1\n",
    "    \n",
    "    return answer"
   ]
  }
