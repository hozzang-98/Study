    "def solution(my_str, n):\n",
    "    \n",
    "    answer = []\n",
    "    \n",
    "    v = len(my_str) // n\n",
    "    mod = len(my_str) % n\n",
    "    \n",
    "    \n",
    "    for i in range(v):\n",
    "        answer.append(my_str[i*n:(i+1)*n])\n",
    "    \n",
    "    if mod != 0: # n의 배수가 아니어서 나머지 문자열이 있다면\n",
    "        answer.append(my_str[v*n:])\n",
    "\n",
    "    return answer"
   ]
  }
