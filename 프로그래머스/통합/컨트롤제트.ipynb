    "def solution(s):\n",
    "    answer = 0\n",
    "    temp = s.split(' ')\n",
    "    for i in range(len(temp)):\n",
    "        if temp[i] != 'Z':\n",
    "            answer += int(temp[i])\n",
    "        else:\n",
    "            answer -= int(temp[i-1])\n",
    "    return answer"
   ]
